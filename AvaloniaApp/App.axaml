<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:dialogHost="using:DialogHostAvalonia"
             x:Class="AvaloniaApp.App"
             xmlns:presentation="using:AvaloniaApp.Presentation"
             xmlns:vmusercontrols="using:AvaloniaApp.Presentation.ViewModels.UserControls" 
             xmlns:vusercontrols="using:AvaloniaApp.Presentation.Views.UserControls"
             xmlns:vm="using:AvaloniaApp.Presentation.ViewModels"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <presentation:ViewLocator/>
    </Application.DataTemplates>
  
    <Application.Styles>
        <FluentTheme/>
        <dialogHost:DialogHostStyles />
    </Application.Styles>
</Application>